{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Punchcard Visualization like on Github. The day on y-axis uses a custom order from Monday to Sunday.  The sort property supports both full day names (e.g., 'Monday') and their three letter initials (e.g., 'mon') -- both of which are case insensitive.",
    "width": 400,
    "height": 300,
    "data": { "url": "https://raw.githubusercontent.com/Rafaquat/FIT_3179_A2/main/Data_Vis_2/data/processed_data/bubbleChartData/Common_Depression_Traits.csv" },
    "mark": "circle",
    "encoding": {
        "y": {
            "field": "Y-Axis",
            "type": "quantitative"
        },
        "x": {
            "field": "X-Axis",
            "type": "quantitative"
        },
        "size": {
            "field": "Depression Cases",
            "type": "quantitative",
            "aggregate": "sum",
            "scale": { "rangeMin": "10", "rangeMax": 5000 },
            "legend": null
        },
        "color": {
            "field": "Causes",
            "type": "nominal",
            "title": "Common Traits"
        }
    }
}